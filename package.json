{
  "name": "remix-cloudflare-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"pnpm run start\"",
    "dev:remote": "remix dev --manual -c \"pnpm run start:remote\"",
    "format": "biome format --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-12-29 ./public",
    "start:remote": "wrangler pages dev --local=false --compatibility-date=2023-12-29 ./public",
    "typecheck": "tsc",
    "deploy": "pnpm run build && wrangler pages deploy ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.4.1",
    "@remix-run/cloudflare-pages": "^2.4.1",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "isbot": "3.7.1-deprecated",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@remix-run/dev": "^2.4.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "wrangler": "3.22.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}